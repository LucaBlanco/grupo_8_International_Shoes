<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %> 
</head>
<body>
    <%- include("../partials/nav")  %>

    <% if (users.length>0) { %>
    <div class="center">
        <h1>Listado de usuarios</h1>
    <% users.forEach(user => { %>

        <div class="">
            <a href="/users/<%= user.id %>"><img src="/<%= user.image %>" width="300" alt=""></a>
            <div class="">
                <p class=""> <%= user.firstName %> <%= user.lastName %> | <%= user.email %> | <%= user.userName %> | <%= user.birthDate %> | <%= user.country %> | <%= user.city %> | <%= user.image %> </p>
            </div>
        </div>

    <% }) %>
    </div>
    <% } else { %>
        <h2>No se encontro ningun user</h2>
     <% } %>      
     <%- include("../partials/footer") %> 
</body>
</html>